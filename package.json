{
  "name": "mweb",
  "description": "Material Web Freedom Version is a dependency-free single CDN bundle variation of Material Web.",
  "version": "0.0.1",
  "homepage": "https://github.com/jfbrennan/mw-",
  "repository": {
    "type": "git",
    "url": "https://github.com/jfbrennan/mw-"
  },
  "bugs": "https://github.com/jfbrennan/mw-/issues",
  "files": [
    "/dist"
  ],
  "license": "MIT",
  "contributors": [
    {
      "name": "Jordan Brennan",
      "url": "https://github.com/jfbrennan"
    }
  ],
  "keywords": [
    "material",
    "material design",
    "design system",
    "components",
    "web components",
    "freedom"
  ],
  "browserslist": [
    "last 1 version",
    "> 5%",
    "not dead"
  ],
  "scripts": {
    "watch": "nodemon --watch src --ext css,js --exec npm run build",
    "build": "rm -r dist; mkdir dist && npm run css && npm run js",
    "css": "postcss src/mw-all.css | lightningcss --nesting --minify -o ./dist/mw-.css /dev/stdin",
    "css:custom-properties": "cp -R src/custom-properties.css dist",
    "icons": "cp -R src/m-.woff2 dist/m-.woff2",
    "js": "terser ./src/*.js --mangle -o ./dist/mw-.js"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.1.4",
    "lightningcss-cli": "^1.21.7",
    "postcss": "^8.4.29",
    "postcss-cli": "^10.1.0",
    "postcss-custom-properties": "^13.3.0",
    "postcss-import": "^15.1.0",
    "terser": "^5.19.4"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
